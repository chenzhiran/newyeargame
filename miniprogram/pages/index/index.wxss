/**index.wxss**/
.first-wrapper{
  width: 100%;
  height: 100%;
  background: #A62116;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
}

/* 背景图片 */
.first-bg{
  width: 100%;
  height: 100%;
  position: relative;
  z-index: 10;
}

/* 跳转按钮 */
.test-button{
  width: 300rpx;
  height: 130rpx;
  color:white!important;
  border: #A62116;
  border-radius: 44rpx;
  background:linear-gradient(90deg,rgb(241, 73, 58),#A62116);
  box-shadow: 0px 6px 10px 0px rgb(241, 136, 126); 
  font-size: 60rpx;
  display: inline-block;
  line-height: 130rpx;
  position: fixed;
  bottom:200rpx;
  left: 230rpx;
  z-index: 10;
}

.comeon-bg{
  width: 300rpx;
  height: 130rpx;
  position: fixed;
  bottom:200rpx;
  left: 230rpx;
  z-index: 10;
}